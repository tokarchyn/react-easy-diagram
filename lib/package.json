{
  "name": "react-easy-diagram",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tokarchyn/react-easy-diagram.git"
  },
  "keywords": [
    "diagram",
    "diagrams",
    "react",
    "typescript",
    "flowchart",
    "nodes",
    "links"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "dependencies": {
    "@use-gesture/react": "^10.2.21",
    "mobx": "^6.4.0",
    "mobx-react-lite": "^3.4.0"
  },
  "scripts": {
    "build:prod": "yarn prebuild && yarn clear && rollup -c --environment NODE_ENV:production",
    "build:dev": "yarn prebuild && yarn clear && rollup -c",
    "start": "rollup -c -w",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "clear": "yarn run del \"dist/*\"",
    "prebuild": "yarn cti entrypoint ./src -o index.ts -b",
    "test": "jest",
    "test-with-coverage": "jest --coverage"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "autoprefixer": "^10.2.1",
    "babel-jest": "^27.0.6",
    "copy-and-watch": "^0.1.5",
    "copyfiles": "^2.4.0",
    "create-ts-index": "^1.13.6",
    "del-cli": "^3.0.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "mobx-logger": "^0.7.1",
    "postcss": "^8.2.4",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.40.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-sizes": "^1.0.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^27.0.4",
    "ttypescript": "^1.5.12",
    "types": "^0.1.1",
    "typescript": "~4.0.5"
  },
  "peerDependencies": {
    "@types/react": "16 || 17 || 18",
    "@types/react-dom": "16 || 17 || 18",
    "react": "16 || 17 || 18",
    "react-dom": "16 || 17 || 18"
  }
